INST_MULH	./define.svh	/^`define INST_MULH   3'b001$/;"	d
INST_MULHSU	./define.svh	/^`define INST_MULHSU 3'b010$/;"	d
INST_MULHU	./define.svh	/^`define INST_MULHU  3'b011$/;"	d
